@model List<TCC.Models.Account>

@{
    ViewData["Title"] = "Index";
}

@foreach (var account in Model)
{
    <p>Id: @account.Id</p>
    <p>UserId: @account.UserId</p>
    <p>Name: @account.Name</p>
    <p>Balance: @account.Balance</p>
    <p>CreationDate: @account.CreationDate</p>
    <p>------------------------------------------------</p>
}

